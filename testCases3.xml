<?xml version="1.0" encoding="UTF-8" standalone="yes" ?> 
<testCases>
	<testCase id="type.TEXT.check">
		<typoscript>
		<![CDATA[
bla = TEXT
bla.]]>
		</typoscript>
		<check type="assertFirstProposal">
			value
		</check>
	</testCase>
	<testCase id="type.IMAGE.check">
		<typoscript>
		<![CDATA[
bla = IMAGE
bla.]]>
		</typoscript>
		<check type="assertFirstProposal">
			altText
		</check>
	</testCase>
	<testCase id="override1">
		<typoscript>
		<![CDATA[
bla = IMAGE
bla = TEXT
bla.]]>
		</typoscript>
		<check type="assertFirstProposal">
			value
		</check>
	</testCase>
	<testCase id="simpleCopy">
		<typoscript>
		<![CDATA[
bla = TEXT
blu < bla
blu.]]>
		</typoscript>
		<check type="assertFirstProposal">
			value
		</check>
	</testCase>
	<testCase id="reference1">
		<typoscript>
		<![CDATA[
bla = TEXT
blu =< bla
blu.]]>
		</typoscript>
		<check type="assertFirstProposal">
			value
		</check>
	</testCase>
	<testCase id="reference2">
		<typoscript>
		<![CDATA[
bla = TEXT
blu =< bla
blu.aaa = xxx
bla.]]>
		</typoscript>
		<check type="assertFirstProposal">
			aaa
		</check>
	</testCase>
	<testCase id="nestedType">
		<typoscript>
		<![CDATA[
bla = TEXT
bla.aaa = IMAGE
bbb = TEXT
bbb.value = fuf
bla.aaa.]]>
		</typoscript>
		<check type="assertFirstProposal">
			altText
		</check>
	</testCase>
	<testCase id="nestedType2">
		<typoscript>
		<![CDATA[
bla = TEXT
bla{
  aaa = IMAGE
}
bbb = TEXT
bbb.value = fuf
bla.aaa.]]>
		</typoscript>
		<check type="assertFirstProposal">
			altText
		</check>
	</testCase>
	<testCase id="delete1">
		<typoscript>
		<![CDATA[
temp.myImage = IMAGE
temp.myImage >
temp.
]]>
		</typoscript>
		<check type="assertNoProposals"></check>
	</testCase>
	<testCase id="delete2">
		<typoscript>
		<![CDATA[
temp.myImage = IMAGE
temp.myImage.test1.test2 = bla
temp.myImage >
temp.myImage.test1.
]]>
		</typoscript>
		<check type="assertNoProposals"></check>
	</testCase>
	<testCase id="relativePath1">
		<typoscript>
		<![CDATA[
temp.myImage = IMAGE
temp.myImage{
	
]]>
		</typoscript>
		<check type="assertFirstProposal">
			altText
		</check>
	</testCase>
	<testCase id="relativePath2">
		<typoscript>
		<![CDATA[
temp.myImage = IMAGE
temp.myImage{
	file{
		
]]>
		</typoscript>
		<check type="assertFirstProposal">
			altText
		</check>
	</testCase>
	<testCase id="rightSideCompletion">
		<typoscript>
		<![CDATA[
test < lib.
]]>
		</typoscript>
		<check type="assertFirstProposal">
			stdheader
		</check>
	</testCase>
	<testCase id="externalTemplate1">
		<typoscript>
		<![CDATA[
lib.
]]>
		</typoscript>
		<check type="assertFirstProposal">
			stdheader
		</check>
	</testCase>
</testCases>
